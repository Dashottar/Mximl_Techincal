   "source": [
    "def distinctChar(S, n):\n",
    "    s = set()\n",
    " \n",
    "    for i in range(n):\n",
    "        if (S[i] not in s):\n",
    "            s.add(S[i])\n",
    " \n",
    "    return len(s)\n",
    " \n",
    "S = input()\n",
    "n = len(S)\n",
    " \n",
    "print(distinctChar(S, n))"
   ]
